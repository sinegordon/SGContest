@model List<string>
@{
    ViewData["Title"] = "Пользователи";
}

<div class="text-center">
    <h1 class="display-4">Пользователи</h1>
</div>

<div>
    <table class="table table-striped table-hover">
        <tr border="1">
            <td></td>
            <td>Имя</td>
        </tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>Студент</td>
                <td>@user</td>
            </tr>
        }
    </table>

</div>
